 }\r\n    }\r\n  },\r\n  watch: {\r\n    option() {\r\n      this.setOption();\r\n    },\r\n    screenWidth() {\r\n      this.resize();\r\n    },\r\n    collapse() {\r\n      setTimeout(() => {\r\n        this.resize();\r\n      }, 360);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  },\r\n  methods: {\r\n    /* 初始化echarts实例 */\r\n    init() {\r\n      this.chart = echarts.init(this.$refs.chart, EleCharTheme);\r\n      this.setOption();\r\n      this.$emit('done', this.chart);\r\n    },\r\n    /* 绘制图表 */\r\n    setOption(option) {\r\n      if (!this.chart) return;\r\n      this.chart.setOption(option || this.option);\r\n    },\r\n    /* 重置尺寸 */\r\n    resize() {\r\n      if (!this.chart) return;\r\n      this.chart.resize();\r\n    }\r\n  }\r\n}\r\n</script>"]}]}