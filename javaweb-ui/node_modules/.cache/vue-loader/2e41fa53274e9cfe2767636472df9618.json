\r\n    </template>\r\n  </el-submenu>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"EleSidebarItem\",\r\n  props: {\r\n    item: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    path: [String, Number],\r\n    popClass: String\r\n  },\r\n  methods: {\r\n    /* 获取路径，嵌套路由前面加父路径 */\r\n    getPath(url) {\r\n      if (url && (url.indexOf('/') === 0 || url.indexOf('http') === 0)) return url;\r\n      if (this.path && typeof this.path === 'string') {\r\n        if (this.path.lastIndexOf('/') === (this.path.length - 1)) return this.path + (url || '');\r\n        else return this.path + '/' + (url || '');\r\n      }\r\n      return url || '';\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}