AA,QAAA,CAAA,KAAA,CAAA,CAAA,CAAA,OAAA;AACA,WAXA;AAYA,SAdA,EAcA,KAdA,CAcA;AAAA,iBAAA,CAAA;AAAA,SAdA;AAeA,OAlBA,MAkBA;AAAA;AACA,YAAA,OAAA,GAAA,KAAA,QAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAAA;AACA,aAAA,KAAA,CAAA,MAAA,CAAA,kBAAA,CAAA,GAAA,CAAA,EAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA;;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA;AAAA,cAAA,IAAA,EAAA,SAAA;AAAA,cAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAAA,aAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,MAAA;AACA,WAHA,MAGA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,SARA,EAQA,KARA,CAQA,UAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,CAAA,CAAA,OAAA;AACA,SAXA;AAYA;AACA,KAxEA;;AAyEA;AACA,IAAA,SA1EA,qBA0EA,GA1EA,EA0EA;AAAA;;AACA,UAAA,OAAA,GAAA,KAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,CAAA,CADA,CAEA;AACA;;AACA,UAAA,MAAA,GAAA;AAAA,cAAA,GAAA,CAAA,EAAA;AAAA,kBAAA,GAAA,CAAA;AAAA,OAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AAAA,YAAA,IAAA,EAAA,SAAA;AAAA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAAA,WAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,CAAA,CAAA,OAAA;AACA,OAVA;AAWA;AA1FA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"ele-body\">\r\n    <el-card shadow=\"never\">\r\n      <!-- 搜索表单 -->\r\n      <el-form :model=\"table.where\" label-width=\"77px\" class=\"ele-form-search\"\r\n               @keyup.enter.native=\"$refs.table.reload()\" @submit.native.prevent>\r\n        <el-row :gutter=\"15\">\r\n    \r\n          <el-col :md=\"4\" :sm=\"12\">\r\n            <el-form-item label=\"案例名称:\">\r\n              <el-input v-model=\"table.where.name\" placeholder=\"请输入案例名称\" clearable/>\r\n            </el-form-item>\r\n          </el-col>\r\n          <!-- 状态下拉单选 -->\r\n          <el-col :md=\"4\" :sm=\"12\">\r\n            <el-form-item label=\"状态:\">\r\n              <el-select v-model=\"table.where.status\" placeholder=\"请选择状态\" clearable class=\"ele-fluid\">\r\n                <el-option label=\"全部\" :value=\"''\"/>\r\n                <el-option label=\"正常\" :value=\"1\"/>\r\n                <el-option label=\"停用\" :value=\"2\"/>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <!-- 类型下拉单选 -->\r\n          <el-col :md=\"4\" :sm=\"12\">\r\n            <el-form-item label=\"类型:\">\r\n              <el-select v-model=\"table.where.type\" placeholder=\"请选择类型\" clearable class=\"ele-fluid\">\r\n                <el-option label=\"全部\" :value=\"''\"/>\r\n                <el-option label=\"京东\" :value=\"1\"/>\r\n                <el-option label=\"淘宝\" :value=\"2\"/>\r\n                <el-option label=\"拼多多\" :value=\"3\"/>\r\n                <el-option label=\"唯品会\" :value=\"4\"/>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :md=\"6\" :sm=\"12\">\r\n            <div class=\"ele-form-actions\">\r\n              <el-button type=\"primary\" @click=\"$refs.table.reload()\" icon=\"el-icon-search\" class=\"ele-btn-icon\">查询\r\n              </el-button>\r\n              <el-button @click=\"(table.where={})&&$refs.table.reload()\">重置</el-button>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <!-- 操作按钮 -->\r\n      <div class=\"ele-table-tool ele-table-tool-default\">\r\n        <el-button @click=\"showEdit=true\" type=\"primary\" icon=\"el-icon-plus\" class=\"ele-btn-icon\" size=\"small\" v-if=\"permission.includes('sys:test:add')\">添加\r\n        </el-button>\r\n        <el-button @click=\"remove()\" type=\"danger\" icon=\"el-icon-delete\" class=\"ele-btn-icon\" size=\"small\" v-if=\"permission.includes('sys:test:dall')\">批量删除\r\n        </el-button>\r\n      </div>\r\n      <!-- 数据表格 -->\r\n      <ele-data-table ref=\"table\" :config=\"table\" :choose.sync=\"choose\" height=\"calc(100vh - 315px)\" highlight-current-row>\r\n        <template slot-scope=\"{index}\">\r\n          <el-table-column type=\"selection\" width=\"45\" align=\"center\" fixed=\"left\"/>\r\n          <el-table-column type=\"index\" :index=\"index\" label=\"编号\" width=\"60\" align=\"center\" fixed=\"left\" show-overflow-tooltip/>\r\n          <el-table-column prop=\"name\" label=\"案例名称\" align=\"center\" show-overflow-tooltip min-width=\"200\"/>\r\n          <el-table-column label=\"头像\" min-width=\"60\" align=\"center\">\r\n            <template slot-scope=\"{row}\">\r\n              <el-avatar shape=\"square\" :size=\"35\" :src=\"row.avatar\"/>\r\n            </template>\r\n          </el-table-column>\r\n            <el-table-column prop=\"status\" label=\"状态\" align=\"center\" :resizable=\"false\" min-width=\"120\">\r\n              <template slot-scope=\"{row}\">\r\n                <el-switch v-model=\"row.status\" @change=\"setStatus(row)\" :active-value=\"1\" :inactive-value=\"2\"/>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"type\" label=\"类型\" align=\"center\" min-width=\"100\">\r\n              <template slot-scope=\"{row}\">\r\n                <ele-dot :type=\"['success', 'danger', 'primary', 'warning', 'info'][row.type-1]\" :ripple=\"row.type===0\"\r\n                         :text=\"['京东','淘宝','拼多多','唯品会'][row.type-1]\"/>\r\n              </template>\r\n            </el-table-column>\r\n          <el-table-column prop=\"sort\" label=\"显示顺序\" align=\"center\" show-overflow-tooltip min-width=\"200\"/>\r\n          <el-table-column label=\"出生时间\" align=\"center\" show-overflow-tooltip min-width=\"160\">\r\n            <template slot-scope=\"{row}\">{{ row.birthday | toDateString }}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"添加时间\" align=\"center\" show-overflow-tooltip min-width=\"160\">\r\n            <template slot-scope=\"{row}\">{{ row.createTime | toDateString }}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"更新时间\" align=\"center\" show-overflow-tooltip min-width=\"160\">\r\n            <template slot-scope=\"{row}\">{{ row.updateTime | toDateString }}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\" width=\"130px\" align=\"center\" :resizable=\"false\"  fixed=\"right\">\r\n            <template slot-scope=\"{row}\">\r\n              <el-link @click=\"edit(row)\" icon=\"el-icon-edit\" type=\"primary\" :underline=\"false\" v-if=\"permission.includes('sys:test:edit')\">修改</el-link>\r\n              <el-popconfirm title=\"确定要删除此演示吗？\" @confirm=\"remove(row)\" class=\"ele-action\">\r\n                <el-link slot=\"reference\" icon=\"el-icon-delete\" type=\"danger\" :underline=\"false\" v-if=\"permission.includes('sys:test:delete')\">删除</el-link>\r\n              </el-popconfirm>\r\n            </template>\r\n          </el-table-column>\r\n        </template>\r\n      </ele-data-table>\r\n    </el-card>\r\n    <!-- 编辑弹窗 -->\r\n    <el-dialog :title=\"editForm.id?'修改演示':'修改演示'\" :visible.sync=\"showEdit\" width=\"400px\"\r\n               @closed=\"editForm={}\" :destroy-on-close=\"true\" :lock-scroll=\"false\">\r\n      <el-form :model=\"editForm\" ref=\"editForm\" :rules=\"editRules\" label-width=\"82px\">\r\n        <el-form-item label=\"案例名称:\" prop=\"name\">\r\n          <el-input v-model=\"editForm.name\" placeholder=\"请输入案例名称\" clearable/>\r\n        </el-form-item>\r\n        <el-form-item label=\"头像:\">\r\n          <uploadImage :limit=\"1\" :updDir=\"updDir\" v-model=\"editForm.avatar\"></uploadImage>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态:\">\r\n          <el-radio-group v-model=\"editForm.status\">\r\n            <el-radio :label=\"1\">正常</el-radio>\r\n            <el-radio :label=\"2\">停用</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型:\" prop=\"type\">\r\n          <el-select v-model=\"editForm.type\" placeholder=\"请选择类型\" class=\"ele-block\" clearable>\r\n              <el-option label=\"京东\" :value=\"1\"/>\r\n              <el-option label=\"淘宝\" :value=\"2\"/>\r\n              <el-option label=\"拼多多\" :value=\"3\"/>\r\n              <el-option label=\"唯品会\" :value=\"4\"/>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"显示顺序:\" prop=\"sort\">\r\n          <el-input-number v-model=\"editForm.sort\" controls-position=\"right\" :min=\"0\"\r\n                           placeholder=\"请输入显示顺序\" class=\"ele-fluid ele-text-left\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"出生时间:\" prop=\"birthday\">\r\n          <el-date-picker\r\n                  v-model=\"editForm.birthday\"\r\n                  type=\"datetime\"\r\n                  placeholder=\"选择出生时间\"\r\n                  size=\"small\"\r\n                  value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                  format=\"yyyy-MM-dd HH:mm:ss\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\">\r\n        <el-button @click=\"showEdit=false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">保存</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n  import { mapGetters } from \"vuex\";\r\n  import uploadImage from '@/components/uploadImage'\r\n  export default {\r\n    name: \"SysTest\",\r\n    data() {\r\n      return {\r\n        table: {url: '/test/index', where: {}},  // 表格配置\r\n        choose: [],  // 表格选中数据\r\n        showEdit: false,  // 是否显示表单弹窗\r\n        editForm: {},  // 表单数据\r\n        editRules: {  // 表单验证规则\r\n\r\n        },\r\n        updDir: 'test',\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapGetters([\"permission\"]),\r\n    },\r\n    components: {uploadImage},\r\n    mounted() {\r\n    },\r\n    methods: {\r\n      /* 显示编辑 */\r\n      edit(row) {\r\n        this.$http.get('/test/info/' + row.id).then(res => {\r\n          if (res.data.code === 0) {\r\n            this.editForm = res.data.data;\r\n            this.showEdit = true;\r\n          } else {\r\n            this.$message.error(res.data.msg);\r\n          }\r\n        }).catch(e => {\r\n          this.$message.error(e.message);\r\n        });\r\n      },\r\n      /* 保存编辑 */\r\n      save() {\r\n        this.$refs['editForm'].validate((valid) => {\r\n          if (valid) {\r\n            const loading = this.$loading({lock: true});\r\n            this.$http[this.editForm.id ? 'put' : 'post'](this.editForm.id ? '/test/edit' : '/test/add', this.editForm).then(res => {\r\n              loading.close();\r\n              if (res.data.code === 0) {\r\n                this.showEdit = false;\r\n                this.$message({type: 'success', message: res.data.msg});\r\n                this.$refs.table.reload();\r\n              } else {\r\n                this.$message.error(res.data.msg);\r\n              }\r\n            }).catch(e => {\r\n              loading.close();\r\n              this.$message.error(e.message);\r\n            });\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      },\r\n      /* 删除 */\r\n      remove(row) {\r\n        if (!row) {  // 批量删除\r\n          if (this.choose.length === 0) return this.$message.error('请至少选择一条数据');\r\n          let ids = this.choose.map(d => d.id);\r\n          this.$confirm('确定要删除选中的演示吗?', '提示', {type: 'warning'}).then(() => {\r\n            const loading = this.$loading({lock: true});\r\n            this.$http.delete('/test/delete/' + ids).then(res => {\r\n              loading.close();\r\n              if (res.data.code === 0) {\r\n                this.$message({type: 'success', message: res.data.msg});\r\n                this.$refs.table.reload();\r\n              } else {\r\n                this.$message.error(res.data.msg);\r\n              }\r\n            }).catch(e => {\r\n              loading.close();\r\n              this.$message.error(e.message);\r\n            });\r\n          }).catch(() => 0);\r\n        } else {  // 单个删除\r\n          const loading = this.$loading({lock: true});\r\n          this.$http.delete('/test/delete/' + [row.id]).then(res => {\r\n            loading.close();\r\n            if (res.data.code === 0) {\r\n              this.$message({type: 'success', message: res.data.msg});\r\n              this.$refs.table.reload();\r\n            } else {\r\n              this.$message.error(res.data.msg);\r\n            }\r\n          }).catch(e => {\r\n            loading.close();\r\n            this.$message.error(e.message);\r\n          });\r\n        }\r\n      },\r\n      /* 更改状态 */\r\n      setStatus(row) {\r\n        const loading = this.$loading({lock: true});\r\n        //let params = new FormData();\r\n        // params.append('status', row.status);\r\n        let params = {\"id\":row.id, 'status': row.status};\r\n        this.$http.put('/test/setStatus', params).then(res => {\r\n          loading.close();\r\n          if (res.data.code === 0) {\r\n            this.$message({type: 'success', message: res.data.msg});\r\n          } else {\r\n            this.$message.error(res.data.msg);\r\n          }\r\n        }).catch(e => {\r\n          loading.close();\r\n          this.$message.error(e.message);\r\n        });\r\n      },\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  .ele-block >>> .el-upload, .ele-block >>> .el-upload-dragger {\r\n    width: 100%;\r\n  }\r\n</style>"],"sourceRoot":"src/views/system/test"}]}